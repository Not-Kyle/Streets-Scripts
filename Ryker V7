Commands["magin shotty"] = {
    ["Aliases"] = {"magicshotty", "magicgun"},
    ["Function"] = function(Args)
game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Magic gun";
    Text = "On!";  
    Duration = 1 
	})
local getc = game.Players.LocalPlayer.Backpack.Shotty:GetChildren()


 game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
 if key == "w" then
                      for i,v in pairs (getc) do
    if v.Name == "Union" then
    v.UsePartColor = true 
    wait()
    v.Material = "Neon"
    v.Color = Color3.fromRGB(255,0,0)
    v.Transparency = 0.7

    end
end
 end
end)

 game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
 if key == "a" then
                      for i,v in pairs (getc) do
    if v.Name == "Union" then
    v.UsePartColor = true 
    wait()
    v.Material = "Neon"
    v.Color = Color3.fromRGB(255,255,0)
    v.Transparency = 0.7

    end
end
 end
end)

 game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
 if key == "s" then
                      for i,v in pairs (getc) do
    if v.Name == "Union" then
    v.UsePartColor = true 
    wait()
    v.Material = "Neon"
    v.Color = Color3.fromRGB(48, 255, 0)
    v.Transparency = 0.7

    end
end
 end
end)

game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
 if key == "d" then
                      for i,v in pairs (getc) do
    if v.Name == "Union" then
    v.UsePartColor = true 
    wait()
    v.Material = "Neon"
    v.Color = Color3.fromRGB(0, 255, 249)
    v.Transparency = 0.7

    end
end
 end
end)









end
}

Commands["float"] = {
    ["Aliases"] = {"levitate", "float"},
    ["Function"] = function(Args)

Player = "LocalPlayer"
AnimationId = "313762630"
local Anim = Instance.new("Animation")
Anim.AnimationId = "rbxassetid://"..AnimationId
local k = game.Players[Player].Character.Humanoid:LoadAnimation(Anim)
k:Play() 
k:AdjustSpeed(1)

end
}


-- walkshoot 
if game.PlaceId == 4669040 or 455366377 then
local mt = getrawmetatable(game)
local backup
backup = hookfunction(mt.__newindex, newcclosure(function(self, key, value)
if key == "WalkSpeed" and value < 16 then
value = 16
end
return backup(self, key, value)
end))
end
--end of walkshoot
----noclip
-- Press N to activate

local StealthMode = false -- If game has an anticheat that checks the logs

local Indicator

if not StealthMode then
    local ScreenGui = Instance.new("ScreenGui", game.CoreGui)
    print("NOCLIP: Press N to Activate")
    Indicator = Instance.new("TextLabel", ScreenGui)
    Indicator.AnchorPoint = Vector2.new(0, 1)
    Indicator.Position = UDim2.new(0, 0, 1, 0)
    Indicator.Size = UDim2.new(0, 200, 0, 50)
    Indicator.BackgroundTransparency = 1
    Indicator.TextScaled = true
    Indicator.TextStrokeTransparency = 0
    Indicator.TextColor3 = Color3.new(0, 0, 0)
    Indicator.TextStrokeColor3 = Color3.new(1, 1, 1)
    Indicator.Text = ""
end

local noclip = true
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

local mouse = player:GetMouse()

mouse.KeyDown:Connect(function(key)
    if key == "n" then
        noclip = not noclip

        if not StealthMode then
            Indicator.Text = ""
        end
    end
end)

while true do
    player = game.Players.LocalPlayer
    character = player.Character

    if noclip then
        for _, v in pairs(character:GetDescendants()) do
            pcall(function()
                if v:IsA("BasePart") then
                    v.CanCollide = false
                end
            end)
        end
    end

    game:GetService("RunService").Stepped:wait()
end
