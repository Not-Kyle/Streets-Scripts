                    game.Players.LocalPlayer.Character.Humanoid:findFirstChild("Bullet").Name = "BulletDone"
                end
            end
        end
    end
end)
        end

if TextBox.Text == 'yellow' then
	ThemainGuiFrame.Visible = true
	Open2Frame.Visible = true
        WhiteListframe.Visible = false
         game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Bullet Color";
    Text = "yellow";  
